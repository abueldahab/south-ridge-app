{
	"config_version": "2",
	"platform_version": "v1.3",
	"name": "South Ridge",
	"author": "kris.krause@gmail.com",
	"version": "1.4",
	"description": "South Ridge Community Church",
	"modules": {
		"package_names": {
            "android": "com.camomedia.southridge",
            "ios": "com.camomedia.ios.southridge"
        },
		"launchimage": {
	            "iphone": "img/320x480.png",
	            "iphone-retina": "img/640x960.png",
	            "ipad": "img/768x1004.png",
	            "ipad-landscape": "img/1024x748.png",
	            "android": "img/640x960.png",
	            "android-landscape": "img/1024x748.png"
	        },
	    "orientations": { 
            "default": "any",
            "iphone": "portrait",
            "ipad": "portrait",
            "android": "portrait"
	    },
	    "geolocation": true,
	    "file": true,
		"event": true,
		"is": true,
		"logging": {
			"level": "INFO"
		},
		"request": {
			"permissions": ["*://graph.facebook.com/*", "*://*.vimeo.com/*", "*://*.rackcdn.com/*", "*://*.parse.com/*"]
		},
		"tabs": true,
		"tools": true,
		"topbar": true,
		"tabbar": true,
		"media": true,
		"icons": {
            "android": {
                "36": "img/icons/icon36-android.png",
                "48": "img/icons/icon48-android.png",
                "72": "img/icons/icon72-android.png"
            },
            "ios": {
                "57": "img/icons/icon57-ios.png",
                "72": "img/icons/icon72-ios.png",
                "114": "img/icons/icon114-ios.png"
            }
        }
	}
}
